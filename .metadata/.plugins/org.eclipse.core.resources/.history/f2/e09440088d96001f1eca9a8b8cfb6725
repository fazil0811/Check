package Arrays;

import java.util.HashSet;
import java.util.Set;

public class LongestConsecutiveSequence {
	
	public static void main(String[] args) {
		
		int[] nums = {100, 4, 200, 1, 3, 2};
		
		Set<Integer> a=new HashSet<>();
		
		for(int x : nums) {
			a.add(x);
		}
		System.out.println(a);
	}

}
